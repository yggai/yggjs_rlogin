{
  "name": "yggjs_rlogin",
  "version": "0.1.1",
  "description": "专为 React 登录页面开发的组件库",
  "keywords": [
    "react",
    "login",
    "auth",
    "component",
    "ui",
    "yggjs",
    "rlogin"
  ],
  "author": "源滚滚 <1156956636@qq.com>",
  "license": "PolyForm-Noncommercial-1.0.0",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.5",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@9.0.0"
}
